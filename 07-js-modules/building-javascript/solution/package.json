{
  "name": "weather-bug",
  "version": "0.0.0",
  "description": "A small fullstack javascript application for checking the weather",
  "main": "/src/server.js",
  "type": "module",
  "scripts": {
    "build:dev": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "start:dev": "nodemon ./src/server.js --experimental-modules",
    "start": "node ./src/server.js --experimental-modules",
    "development": "concurrently \"npm run build:dev\" \"npm run start:dev\""
  },
  "keywords": [
    "JavaScript",
    "Lesson",
    "Tutorial",
    "weather",
    "vanilla"
  ],
  "author": "Moon Highway",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "chart.js": "^2.9.4",
    "express": "^4.17.1",
    "weather-js": "^2.0.0"
  }
}
